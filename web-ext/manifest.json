{
    "manifest_version": 2,
    "name": "Feedly filtering and sorting",
    "version": "1.5.4",
    "description": "Enhance the feedly website with advanced filtering and sorting capabilities",
    "homepage_url": "https://github.com/soufianesakhi/feedly-filtering-and-sorting",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://feedly.com/*"
            ],
            "js": [
                "node-creation-observer.js",
                "jquery.min.js",
                "feedly-filtering-and-sorting.js"
            ]
        }
    ],
    "applications": {
        "gecko": {
            "id": "feedly-filtering-and-sorting@mozilla.org"
        }
    }
}